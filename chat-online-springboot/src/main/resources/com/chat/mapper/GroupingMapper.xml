<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.chat.mapper.GroupingMapper">

    <select id="selectAllGroupingById" resultType="com.chat.pojo.entity.Grouping">
        select *
        from user_grouping
        where user_id=#{userId}
    </select>

</mapper>